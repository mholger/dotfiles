Host git.sussexdirectories.*
    IdentityFile ~/.ssh/git-id

Host *.sussexdirectories.com
    IdentityFile ~/.ssh/sussex
    User matt

Host *.optera.net
	IdentityFile ~/.ssh/optera
	User mholger

Host *.paralleltheory.com
	IdentityFile ~/.ssh/optera
	User matt

Match user matt
	IdentityFile ~/.ssh/sussex

Match user mholger
	IdentityFile ~/.ssh/optera

Host *
    IdentityFile ~/.ssh/optera
    IdentityFile ~/.ssh/sussex
